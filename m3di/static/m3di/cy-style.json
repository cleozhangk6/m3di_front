[{
    "selector": "node",
    "style": {
      "shape": "ellipse",
      "width": "50px",
      "height": "50px",
      "background-color": "#605C69",
      "content": "data(gene)",
      "text-valign": "center",
      "text-halign": "center",
      "text-outline-color": "#605C69",
      "text-outline-width": "2px",
      "color": "#fff"}
  }, {
    "selector": "node[?query]",
    "style": {
      "width": "70px",
      "height": "70px"}
  }, {
    "selector": "node:selected",
    "style": {
      "border-width": 8,
      "border-opacity": 0.6,
      "border-color": "#cccccc",
      "background-color": "#77828C"}
  }, {
    "selector": "edge",
    "style": {
      "width": 4,
      "line-color": "#ffbb99",
      "opacity": 0.7}
  }, {
    "selector": "edge[exp > 0]",
    "style": {
      "line-color": "#e6e6e6"}
  }, {
    "selector": "edge[exp > 300]",
    "style": {
      "line-color": "#cccccc",
      "z-index": 10}
  }, {
    "selector": "edge[exp > 600]",
    "style": {
      "line-color": "#b3b3b3",
      "z-index": 20}
  }, {
    "selector": "edge[?type]",
    "style": {
      "opacity": 0.9,
      "z-index": 100}
  }, {
    "selector": "edge[type=\"Model\"]",
    "style": {
      "line-color": "#ffbf80"}
  }, {
    "selector": "edge[type=\"Structure\"]",
    "style": {
      "line-color": "#d96226"}
  }, {
    "selector": "edge:selected",
    "style": {
      "z-index": 200,
      "overlay-padding": "5px",
      "overlay-color": "#77828C",
      "overlay-opacity": 0.3}
  }
]