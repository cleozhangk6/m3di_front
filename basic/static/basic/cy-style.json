[{
    "selector": "node",
    "style": {
      "shape": "ellipse",
      "width": "40px",
      "height": "40px",
      "background-color": "#605C69",
      "border-color": "#605C69",
      "border-width": 4,
      "content": "data(id)",
      "text-valign": "center",
      "text-halign": "center",
      "text-outline-color": "#605C69",
      "text-outline-width": "2px",
      "color": "#fff"}
  }, {
    "selector": "node[idInt=0]",
    "style": {
      "width": "70px",
      "height": "70px"}
  }, {
    "selector": "edge",
    "style": {
      "width": 4,
      "line-color": "#ffeee6"}
  }, {
    "selector": "edge[exp > 0]",
    "style": {
      "line-color": "#ffbb99"}
  }, {
    "selector": "edge[exp > 300]",
    "style": {
      "line-color": "#ff7733"}
  }, {
    "selector": "edge[exp > 600]",
    "style": {
      "line-color": "#cc4400"}
  }
